#ifndef ARM_MCAL_RCC_RCC_INTERFACE_H_
#define ARM_MCAL_RCC_RCC_INTERFACE_H_

#define RCC_AHB1    0
#define RCC_AHB2	1
#define RCC_APB1	2

void MRCC_vInit(void);
//enable the clock
void MRCC_vEnableClock();
//disable the clock
void MRCC_vDisableClock();




#endif /* ARM_MCAL_RCC_RCC_INTERFACE_H_ */
